{
  "applicationId": "js.neutralino.zero",
  "version": "1.0.0",
  "defaultMode": "window",
  "documentRoot": "/ui/dist/",
  "url": "/",
  "port": 3000,
  "enableServer": true,
  "enableNativeAPI": true,
  "nativeAllowList": [
    "app.*",
    "filesystem.*",
    "os.execCommand",
    "os.spawnProcess",
    "os.updateSpawnedProcess",
    "os.getEnv",
    "os.showFolderDialog",
    "os.showOpenDialog",
    "os.getPath"
  ],
  "modes": {
    "window": {
      "title": "ADB Media Player",
      "width": 1280,
      "height": 720,
      "minWidth": 400,
      "minHeight": 200,
      "icon": "/ui/public/icon.png"
    }
  },
  "cli": {
    "binaryName": "ADB Media Player",
    "resourcesPath": "/ui/dist/",
    "extensionsPath": "/extensions/",
    "binaryVersion": "5.4.0",
    "clientVersion": "5.4.0",
    "frontendLibrary": {
      "patchFile": "/ui/index.html",
      "devUrl": "http://localhost:5173",
      "projectPath": "/ui",
      "initCommand": "bun install",
      "devCommand": "bun run dev",
      "buildCommand": "bun run build"
    }
  }
}